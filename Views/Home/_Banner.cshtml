@model IEnumerable<ChanhThu_Store.Models.Banner>
<div class="box-add-products">
    <div class="container">
        <div class="row">
            @foreach (var i in Model)
            {
                <div class="col-lg-6 col-md-6 col-sm-12">
                    <div class="offer-box-products">
                        <img class="img-fluid" src="@i.HinhBanner" alt="" />
                    </div>
                </div>

            }

        </div>
    </div>
</div>
